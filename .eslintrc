{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "airbnb",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "comma-dangle": [2, "never"],
    "max-len": 0,
    "new-cap": 0,
    "react/jsx-filename-extension": 0,
    "react/jsx-no-bind": 1
  }
}